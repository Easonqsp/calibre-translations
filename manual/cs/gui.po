# cd9108abf0b94dc195343d2455627705
#: ../../home/kovid/work/calibre/manual/gui.rst:466
msgid ""
"will be interpreted to have the title: Foundation and Earth and author: "
"Isaac Asimov"
msgstr "bude interpretován, že má název: Foundation and Earth a autora: Isaac Asimov"

# f0ad536deacc42b28cdd4f36d5003474
#: ../../home/kovid/work/calibre/manual/gui.rst:469
