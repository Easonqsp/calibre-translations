msgid "Set the title."
msgstr "Вказати заголовок"

# cd2d1dac08884837bab1c312dd489834
#: ../../cli/ebook-convert.rst:398
msgid "The version of the title to be used for sorting."
msgstr "Версія заголовка, що використовується для сортування."

# 4dbbcacd5c554a4f92f3229dbfde920d
#: ../../cli/ebook-convert.rst:401
msgid "Debug"
msgstr "Зневадження"

# 76789c1905634c81a0d88b61a4d7bf4c
#: ../../cli/ebook-convert.rst:403
